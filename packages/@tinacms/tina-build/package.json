    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "jest": "26.1.0",
    "meow": "^10.0.1",
    "normalize-path": "^3.0.0",
    "rollup-plugin-dts": "^3.0.2",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "ts-jest": "^26.5.3",
    "tsup": "^4.12.5",
    "typescript": "^4.2.2",
    "vite": "^2.4.3",
