{
  "extends": "../../../tsconfig.json",
  "compilerOptions": {
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "noUncheckedIndexedAccess": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "module": "ESNext",
    "lib": [
      "ESNext"
    ],
    "target": "ESNext",
    "types": [
      "vite/client"
    ],
    "paths": {
      "unist-util-visit": [
        "./index.d.ts"
      ] // this mapping is relative to "baseUrl"
    }
  },
  "exclude": [
    "node_modules",
    "dist"
  ],
  "include": [
    "src",
    "vite-env.d.ts"
  ]
}
